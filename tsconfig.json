{
    "compilerOptions": {
      "outDir": "./dist",
      "module": "commonjs",
      "target": "es6",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "typeRoots": ["./node_modules/@types", "./src/types"]
    },
    "include": ["src/**/*.ts", "src/types/**/*.d.ts"],
    "ts-node": {
      "files": true
    },
  }
  